@model IEnumerable<UserIdentity.Identity.ApplicationUser>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<div class="panel panel-primary" >
    <div class="panel panel-heading" >
        Kullanıcılar
    </div>

    <table class="table table-striped">
        <tr>
            <th>Id</th>
            <th>UserName</th>
            <th>Email</th>
        </tr>
        
        @if(Model.Count() == 0) 
        {
            <tr>
               <td colspan="3" class="text-center">
                   Kullanıcı Yok
               </td>
            </tr>
        }
        else
        {
            foreach (var user in Model)
            {
                <tr>
                    <td>@user.Id</td>
                    <td>@user.UserName</td>
                    <td>@user.Email</td>
                </tr>
            }
        }


    </table>
</div>

